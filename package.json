{
  "name": "pg-vue-primevue-tailwindcss",
  "version": "0.0.0",
  "homepage": "https://pg-vue-primevue-tailwindcss.netlify.app",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pg-vue-primevue-tailwindcss.git"
  },
  "scripts": {
    "dev": "vite",
    "now": "npm run build && npm run preview",
    "lighthouse": "npx unlighthouse --no-cache --site https://pg-vue-primevue-tailwindcss.netlify.app",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "vue-devtools": "vue-devtools",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts ./ --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts,html,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "dependencies": {
    "@vueuse/core": "^11.2.0",
    "@vueuse/head": "^2.0.0",
    "lite-youtube-embed": "^0.3.3",
    "ofetch": "^1.4.1",
    "pinia": "^2.2.6",
    "primevue": "^4.2.2",
    "vee-validate": "^4.14.7",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.272",
    "@pinegrow/primevue-plugin": "^3.0.6",
    "@pinegrow/tailwindcss-plugin": "^3.0.35",
    "@pinegrow/vite-plugin": "^3.0.68",
    "@primevue/auto-import-resolver": "^4.2.2",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tsconfig/node18": "^18.2.4",
    "@types/markdown-it-link-attributes": "^3.0.5",
    "@types/node": "^22.9.0",
    "@unocss/preset-icons": "^0.64.1",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/devtools": "^7.6.4",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.15",
    "tailwindcss-primeui": "^0.3.4",
    "typescript": "~5.6.3",
    "unocss": "^0.64.1",
    "unplugin-auto-import": "^0.18.4",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-markdown": "^0.26.2",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.11",
    "vite-plugin-vue-devtools": "^7.6.4",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^2.1.10"
  },
  "web-types": "web-types.json"
}
